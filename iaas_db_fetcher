    #     # Return all three dataframes created in this method
    #     return invalid_number_of_workers_df

    @raise_exception_globally
    def compute_standby_info(self, analysis_df):
        analysis_df = analysis_df.copy()
        conditions = [(~ analysis_df['resource_name_non_dr'].isna())
                      &
                      (~ analysis_df['resource_name_dr'].isna())
                      &
                      (analysis_df['vm_sku_cost_dr'] < analysis_df['vm_sku_cost_non_dr']),
                      (~ analysis_df['resource_name_non_dr'].isna())
                      &
                      (~ analysis_df['resource_name_dr'].isna())
                      &
                      (analysis_df['vm_sku_cost_dr'] == analysis_df['vm_sku_cost_non_dr']),
                      (~ analysis_df['resource_name_non_dr'].isna())
                      &
                      (~ analysis_df['resource_name_dr'].isna())
                      &
                      (analysis_df['vm_sku_cost_dr'] > analysis_df['vm_sku_cost_non_dr']),
                      (analysis_df['resource_name_non_dr'].isna())
                      |
                      (analysis_df['resource_name_dr'].isna())
                      ]
        choices = ["warm", "hot", "invalid", "cannot determine"]
        analysis_df['standby_mode'] = np.select(conditions, choices)
        # Rank the app service plans within subscription
        conditions = [analysis_df['standby_mode'] == "warm",
                      analysis_df['standby_mode'] == "hot",
                      analysis_df['standby_mode'] == "invalid",
                      analysis_df['standby_mode'] == "cannot determine"
                      ]
        choices = [1, 2, 3, 4]
        analysis_df['standby_rank'] = np.select(conditions, choices)
        # Further rank the data so that the highest of all 4 is taken for every mots
        # Hence, if two app service plans are warm and one is hot, value of warm gets assigned to mots
        # Hence, if three app service plans are hot and one is invalid, value of hot gets assigned to mots
        # Hence, if three app service plans are warm and one is cannot determine, value of warm gets assigned to mots
        analysis_df['standby_priority'] = analysis_df.groupby(['mots_id'])['standby_rank']. \
            rank(method="first", ascending=True)
        standby_df = analysis_df.loc[analysis_df['standby_priority'] == 1][['mots_id', 'standby_mode']]
        return standby_df

    @raise_exception_globally
    def derive_application_standby_info(self, df):
        # Only compute Hot/Warm standby information for PROD subscriptions
        df = df.loc[df['subscription_type'] == 'PROD']
        # Create dataframe with dr regions
        dr_df = df.loc[df.dr_flag == 1]
        dr_df = dr_df.copy()
        # Derive resource key by removing dr and region names from resource name
        if not dr_df.empty:
            dr_df['resource_key'] = dr_df.apply(self.format_dr_resource_name_column, axis=1)
        else:
            dr_df['resource_key'] = ""
        # dr_df.to_csv("dr_df.csv")
        """
        dr_df['resource_key'] = dr_df['resource_name'].str.replace("-dr-", "").\
                                                              replace(dr_df['location'], "")
        """
        # Create dataframe with non dr regions
        non_dr_df = df.loc[df.dr_flag == 0]
        non_dr_df = non_dr_df.copy()
        # Derive resource key by removing prod, dev, nprd, test and region names from resource name
        if not non_dr_df.empty:
            non_dr_df['resource_key'] = non_dr_df.apply(self.format_nondr_resource_name_column, axis=1)
        else:
            non_dr_df['resource_key'] = ""
        # non_dr_df.to_csv("non_dr_df.csv")
        # Join non dr with dr df
        join_df = non_dr_df.merge(dr_df,
                                  on=["mots_id", "subscription_id", "resource_key"],
                                  how="outer",
                                  suffixes=['_non_dr', '_dr'])
        """ Making below a part of common method

        # if both dr and non dr app service plans are present
        # worker size of dr is less than prod, then mark as warm standby
        # if both dr and non dr app service plans are present
        # worker size of dr is equal to prod, then mark as hot standby
        # if both dr and non dr app service plans are present
        # worker size of dr is greater than prod, then mark as invalid standby
        # If either dr or non dr app service plan is present, mark as cannot be determined
        conditions = [(~ join_df['resource_name_non_dr'].isna())
                      &
                      (~ join_df['resource_name_dr'].isna())
                      &
                      (join_df['cost_dr'] < join_df['cost_non_dr']),
                      (~ join_df['resource_name_non_dr'].isna())
                      &
                      (~ join_df['resource_name_dr'].isna())
                      &
                      (join_df['cost_dr'] == join_df['cost_non_dr']),
                      (~ join_df['resource_name_non_dr'].isna())
                      &
                      (~ join_df['resource_name_dr'].isna())
                      &
                      (join_df['cost_dr'] > join_df['cost_non_dr']),
                      (join_df['resource_name_non_dr'].isna())
                      |
                      (join_df['resource_name_dr'].isna())
                      ]
        choices = ["warm", "hot", "invalid", "cannot determine"]
        join_df['standby_mode'] = np.select(conditions, choices)
        # Rank the app service plans within subscription
        conditions = [join_df['standby_mode'] == "warm",
                      join_df['standby_mode'] == "hot",
                      join_df['standby_mode'] == "invalid",
                      join_df['standby_mode'] == "cannot determine"
                      ]
        choices = [1, 2, 3, 4]
        join_df['standby_rank'] = np.select(conditions, choices)
        # Further rank the data so that the highest of all 4 is taken for every mots
        # Hence, if two app service plans are warm and one is hot, value of warm gets assigned to mots
        # Hence, if three app service plans are hot and one is invalid, value of hot gets assigned to mots
        # Hence, if three app service plans are warm and one is cannot determine, value of warm gets assigned to mots
        join_df['standby_priority'] = join_df.groupby(['mots_id'])['standby_rank']. \
            rank(method="first", ascending=True)
        mots_standby_df = join_df.loc[join_df['standby_priority'] == 1][['mots_id', 'standby_mode']]
        """
        mots_standby_df = self.compute_standby_info(join_df)
        # Identify app service plans with invalid standby and create invalid_standby_df
        invalid_standby_df = self.compute_invalid_standby_info(join_df)
        # invalid_number_of_workers_df = self.compute_invalid_number_of_workers_info(join_df)
        # Separate MOTS for which standby mode can be determined
        # and MOTS for which standby mode cannot be determined
        determined_mots_standby_df = mots_standby_df.loc[mots_standby_df['standby_mode'] != 'cannot determine']
        non_determined_mots_standby_df = mots_standby_df.loc[mots_standby_df['standby_mode'] == 'cannot determine']
        non_determined_mots_standby_list = non_determined_mots_standby_df['mots_id'].tolist()
        non_determined_mots_standby_analysis_df = df.loc[df['mots_id'].isin(non_determined_mots_standby_list)]
        reevaulated_mots_standby_df, \
            reevaluated_invalid_standby_df = self.reevaulate_mots_standby_df(
            non_determined_mots_standby_analysis_df)
        reevaulated_mots_standby_list = reevaulated_mots_standby_df['mots_id'].tolist()
        non_determined_mots_standby_df = non_determined_mots_standby_df.loc[~non_determined_mots_standby_df['mots_id'].
        isin(reevaulated_mots_standby_list)]
        # Combine with re-evaluated dataframes
        mots_standby_df = pd.concat(
            [determined_mots_standby_df, reevaulated_mots_standby_df, non_determined_mots_standby_df],
            axis=0)
        invalid_standby_df = pd.concat([invalid_standby_df, reevaluated_invalid_standby_df],
                                       axis=0)
        # invalid_number_of_workers_df = pd.concat(
        #     [invalid_number_of_workers_df, reevaluated_invalid_number_of_workers_df],
        #     axis=0)
        return mots_standby_df, invalid_standby_df

    def compute_dr_flag(self, final_df):
        self.logger.info("Computing DR flag")
        self.logger.info(f"Columns in returned fetch data after compute_dr_flag final df DataFrame: {final_df.columns.tolist()}")

        conditions = [final_df['subscription_name'].str.contains("-PROD-"),
                      final_df['subscription_name'].str.contains("-NPRD-")]
        choices = ["PROD", "NPROD"]
        final_df['subscription_type'] = np.select(conditions, choices, default=0)
        analysis_df = final_df.groupby(['mots_id', 'subscription_type', 'location']). \
            agg({'cpu_utilization': 'sum'}). \
            reset_index()
        # Assign rank of 1 to app service plan within a subscription having the highest utilization
        analysis_df['dr_rank'] = analysis_df.groupby(['mots_id', 'subscription_type'])['cpu_utilization']. \
            rank(method="first", ascending=False)

        conditions = [analysis_df['dr_rank'] == 1]
        choices = [0]
        analysis_df['dr_flag_as_per_cpu_utilization'] = np.select(conditions, choices, default=1)
        # Create a copy of main dataframe
        analysis_2_df = final_df.copy()
        self.logger.info(f"Columns in returned compute_dr_flag analysis_2_df DataFrame: {analysis_2_df.columns.tolist()}")
        conditions = [analysis_2_df['resource_group'].str.contains("-dr-"),
                      analysis_2_df['resource_group'].str.contains("-prod-")]
        choices = [1, 0]
        analysis_2_df['dr_flag_as_per_naming_convention'] = np.select(conditions, choices, default=-1)
        # In case any one resource group does not follow prod or dr naming convention
        # But at least one  within subscription does follow it, take that value
        # Hence, take max of dr flag
        analysis_2_df = analysis_2_df.groupby(['mots_id', 'subscription_type', 'location']). \
            agg({'dr_flag_as_per_naming_convention': 'max'}). \
            reset_index()
        # Merge the final dataframe with aggregated dataframe to fetch the dr flag
        final_df = final_df.merge(analysis_df[['mots_id',
                                               'subscription_type',
                                               'location',
                                               'dr_flag_as_per_cpu_utilization']],
                                  on=['mots_id', 'subscription_type', 'location'],
                                  how="left")
        # Merge the final dataframe with aggregated dataframe to fetch the dr flag
        final_df = final_df.merge(analysis_2_df[['mots_id',
                                                 'subscription_type',
                                                 'location',
                                                 'dr_flag_as_per_naming_convention']],
                                  on=['mots_id', 'subscription_type', 'location'],
                                  how="left")
        # If resource group follows dr or prod naming convention, take that as dr flag
        # else take dr_flag_as_per_cpu_utilization as dr_flag
        conditions = [final_df['dr_flag_as_per_naming_convention'] != -1,
                      final_df['dr_flag_as_per_naming_convention'] == -1
                      ]
        choices = [final_df['dr_flag_as_per_naming_convention'],
                   final_df['dr_flag_as_per_cpu_utilization']
                   ]
        final_df['dr_flag'] = np.select(conditions, choices, default=0)

        # Find for only prod subscriptions, if dr flag as per naming convention marks all
        # the regions as prod, then use cpu utilization
        dr_max_flag_df = final_df.loc[final_df['subscription_type'] == 'PROD'].groupby(['mots_id']).agg(
            {'dr_flag': 'max'}).reset_index()
        # Rename column to be used in the next step
        dr_max_flag_df = dr_max_flag_df.rename(columns={'dr_flag': 'max_dr_flag'})
        # Populate max dr flag for every mots id in final df
        final_df = final_df.merge(dr_max_flag_df[['mots_id', 'max_dr_flag']],
                                  on=['mots_id'],
                                  how="inner")
        # If max flag is 0 for a mots(prod subscriptions), use the dr flag as per cpu utilization
        # If max flag is 0 and there are more than one region for a MOTS,
        # This means app service plans in both primary and dr region have -prod- in their name
        conditions = [(final_df['max_dr_flag'] == 0) &
                      (final_df['subscription_type'] == 'PROD'),
                      (final_df['max_dr_flag'] != 0) &
                      (final_df['subscription_type'] == 'PROD')
                      ]
        choices = [final_df['dr_flag_as_per_cpu_utilization'], final_df['dr_flag']]
        final_df['dr_flag'] = np.select(conditions, choices, default=final_df['dr_flag'])
        return final_df

    @raise_exception_globally
    def generate_measures_based_on_dr_flag(self, final_df):
        # If region has been identified as dr region but has higher utilization
        # assign dr_high_cpu_utilization_flag to 1
        conditions = [(final_df['dr_flag_as_per_cpu_utilization'] == 0) &
                      (final_df['dr_flag'] == 1)]
        choices = [1]
        final_df['dr_high_cpu_utilization_flag'] = np.select(conditions, choices, default=0)
        # Compute standby mode of application
        # Compute invalid standby configuration in dr region
        # Compute invalid number of worker nodes in dr region
        mots_standby_df, invalid_standby_df = self.derive_application_standby_info(
            final_df)
        # mots_standby_df.to_csv("mots_standby_df.csv")
        # Generate unique combinations of subscription id, location and dr flag
        analysis_df = final_df[['mots_id',
                                'subscription_type',
                                'location',
                                'dr_flag']].drop_duplicates()
        # Aggregate number of workers and cpu utilization group by subscription and location
        aggregated_df = final_df.groupby(['mots_id',
                                          'subscription_type',
                                          'location']). \
            agg({
                 'cpu_utilization': 'sum'
                 }).reset_index()
        # Take those subscription and locations that are dr regions
        analysis_dr_df = analysis_df.loc[analysis_df['dr_flag'] == 1]
        # Rename aggregated columns
        aggregated_dr_df = aggregated_df.copy().rename(
            columns={
                     'cpu_utilization': 'cpu_utilization_dr',
                     })
        # Join with dr dataframe to get aggregated values for dr regions
        aggregated_dr_df = aggregated_dr_df.merge(analysis_dr_df,
                                                  on=['mots_id', 'subscription_type', 'location'],
                                                  how="inner")
        # Rename location column as a dr region column
        aggregated_dr_df = aggregated_dr_df.rename(columns={'location': 'location_dr'})
        # In case there are multiple dr regions, combine their values and aggregated measures
        aggregated_dr_df = aggregated_dr_df.groupby(['mots_id', 'subscription_type']). \
            agg({
                 'cpu_utilization_dr': 'sum',
                 'location_dr': lambda x: ", ".join(x)}). \
            reset_index()
        # Take those subscription and locations that are non dr regions
        analysis_active_df = analysis_df.loc[analysis_df['dr_flag'] == 0]
        # Rename aggregated columns
        aggregated_active_df = aggregated_df.copy().rename(
            columns={
                     'cpu_utilization': 'cpu_utilization_non_dr'
                     })
        # Join with non dr dataframe to get aggregated values for dr regions
        aggregated_active_df = aggregated_active_df.merge(analysis_active_df,
                                                          on=['mots_id', 'subscription_type', 'location'],
                                                          how="inner")
        aggregated_active_df = aggregated_active_df.rename(columns={'location': 'location_non_dr'})
        # In case there are multiple dr regions, combine their values and aggregated measures
        aggregated_active_df = aggregated_active_df.groupby(['mots_id', 'subscription_type']). \
            agg({
                 'cpu_utilization_non_dr': 'sum',
                 'location_non_dr': lambda x: ", ".join(x)}). \
            reset_index()
        # Combine final dataframe with aggregated dr data
        final_df = final_df.merge(aggregated_dr_df[['mots_id',
                                                    'subscription_type',
                                                    'location_dr',

                                                    'cpu_utilization_dr']],
                                  on=["mots_id", "subscription_type"],
                                  how="left")
        # Combine final dataframe with aggregated active data
        final_df = final_df.merge(aggregated_active_df[['mots_id',
                                                        'subscription_type',
                                                        'location_non_dr',

                                                        'cpu_utilization_non_dr']],
                                  on=["mots_id", "subscription_type"],
                                  how="left")
        # Count distinct number of regions per subscription
        region_count_df = final_df.groupby(['mots_id', 'subscription_type'])['location'].nunique().reset_index()
        region_count_df = region_count_df.rename(columns={'location': 'location_count'})
        # Merge this count with main dataframe
        final_df = final_df.merge(region_count_df,
                                  on=["mots_id", "subscription_type"],
                                  how="inner")
        # region_count_df.to_csv("region_count_df.csv")
        # Merge standby info for mots with main dataframe
        # Same value gets copied to Non prod subscription as that data is not relevant for hot/warm standby rules
        final_df = final_df.merge(mots_standby_df,
                                  on=["mots_id"],
                                  how="inner")
        # In case standby mode cannot be determined but resource group contains prod,
        # that means resource group within subscription does not contain dr but only prod
        # Hence, missing dr flag is set as 1
        conditions = [(final_df['standby_mode'] == "cannot determine") &
                      (final_df['resource_group'].str.contains("-prod-"))]
        choices = [1]
        final_df['dr_missing_flag'] = np.select(conditions, choices, default=0)
        # In case standby mode cannot be determined but resource group contains dr,
        # that means resource group within subscription does not contain prod but only dr
        # Hence, missing prod flag is set as 1
        conditions = [(final_df['standby_mode'] == "cannot determine") &
                      (final_df['resource_group'].str.contains("-dr-"))]
        choices = [1]
        final_df['prod_missing_flag'] = np.select(conditions, choices, default=0)
        # Merge standby info for mots with main dataframe
        final_df = final_df.merge(invalid_standby_df,
                                  on=["mots_id",
                                      "subscription_id",
                                      "resource_name"],
                                  how="left")
        # Merge invalid workers for mots with main dataframe
        # final_df = final_df.merge(invalid_number_of_workers_df,
        #                           on=["mots_id",
        #                               "subscription_id",
        #                               "resource_name"],
        #                           how="left")
        # Mark records with non dr workers greater than dr workers by more than 30 percentage
        # final_df['workers_delta_more_than_30_percentage'] = ((final_df['current_number_of_workers_non_dr'] -
        #                                                       final_df['current_number_of_workers_dr']) /
        #                                                      final_df['current_number_of_workers_non_dr']) > 0.3
        # # Mark records with non dr workers greater than dr workers by more than 20 percentage
        # final_df['workers_delta_more_than_20_percentage'] = ((final_df['current_number_of_workers_non_dr'] -
        #                                                       final_df['current_number_of_workers_dr']) /
        #                                                      final_df['current_number_of_workers_non_dr']) > 0.2
        final_df['cpu_utilization_delta_more_than_30_percentage'] = ((final_df['cpu_utilization_non_dr'] -
                                                                      final_df['cpu_utilization_dr']) /
                                                                     final_df['cpu_utilization_non_dr']) > 0.3
        # Change the datatype of boolean fields to int values
        # final_df['workers_delta_more_than_30_percentage'] = final_df['workers_delta_more_than_30_percentage'].astype(
        #     int)
        # final_df['workers_delta_more_than_20_percentage'] = final_df['workers_delta_more_than_20_percentage'].astype(
        #     int)
        final_df['cpu_utilization_delta_more_than_30_percentage'] = final_df[
            'cpu_utilization_delta_more_than_30_percentage'].astype(int)
        # Join with rt code
        final_df = final_df.merge(self.rt_values_df[['mots_id', 'rt_code']],
                                  on=['mots_id'],
                                  how="left")
        return final_df

    @raise_exception_globally
    def compute_derived_rt_value(self, final_df):
        # Keep the fields required to calculate RTI value of '0A', '1A' or '1B'.
        analysis_0a_1a_1b_df = final_df[['mots_id',
                                         'subscription_type',
                                         'cpu_utilization_dr',
                                         'cpu_utilization_non_dr',
                                         'dr_exercise_time']].drop_duplicates()
        # Apply the conditions to calculate RTI value of 0A, 1A or 1B.
        conditions = [
            (analysis_0a_1a_1b_df['cpu_utilization_dr'] >= 0.5 * analysis_0a_1a_1b_df['cpu_utilization_non_dr']) &
            (analysis_0a_1a_1b_df['subscription_type'] == "PROD"),
            (analysis_0a_1a_1b_df['cpu_utilization_dr'] < 0.05 * analysis_0a_1a_1b_df['cpu_utilization_non_dr']) &
            (analysis_0a_1a_1b_df['subscription_type'] == "PROD") & (analysis_0a_1a_1b_df['dr_exercise_time'] <= 60),
            (analysis_0a_1a_1b_df['cpu_utilization_dr'] < 0.05 * analysis_0a_1a_1b_df['cpu_utilization_non_dr']) &
            (analysis_0a_1a_1b_df['subscription_type'] == "PROD") & (analysis_0a_1a_1b_df['dr_exercise_time'] > 60)
        ]
        choices = ['0A', '1A', '1B']
        analysis_0a_1a_1b_df['derived_rti'] = np.select(conditions, choices, default=0)
        # Keep only the records where the RTI value of '0A', '1A' or '1B' is calculated
        analysis_0a_1a_1b_df = analysis_0a_1a_1b_df.loc[analysis_0a_1a_1b_df.derived_rti.isin(['0A', '1A', '1B'])]
        # Keep only the required fields
        analysis_0a_1a_1b_df = analysis_0a_1a_1b_df[['mots_id',
                                                     'subscription_type',
                                                     'derived_rti',
                                                     'dr_exercise_time']]

        analysis_0a_1a_1b_df.to_csv("analysis_0a_1a_1b_df.csv")
        analysis_2a_2b_df = final_df[['mots_id',
                                      'subscription_type',
                                      'dr_flag',
                                      'state_flag'
                                      ]]
        analysis_2a_2b_df = analysis_2a_2b_df.groupby(['mots_id',
                                                       'subscription_type',
                                                       'dr_flag']).agg({'state_flag': 'max'}).reset_index()
        conditions = [(analysis_2a_2b_df['dr_flag'] == 1) &
                      (analysis_2a_2b_df['state_flag'] == 0) &
                      (analysis_2a_2b_df['subscription_type'] == "PROD")]
        choices = ['2A/2B']
        analysis_2a_2b_df['derived_rti'] = np.select(conditions, choices, default=0)
        analysis_2a_2b_df = analysis_2a_2b_df.loc[analysis_2a_2b_df.derived_rti.isin(['2A/2B'])]
        # Keep only the required fields
        analysis_2a_2b_df = analysis_2a_2b_df[['mots_id',
                                               'subscription_type',
                                               'derived_rti']]
        analysis_2a_2b_df.to_csv("analysis_2a_2b_df.csv")
        analysis_3a_df = final_df[['mots_id',
                                   'subscription_type',
                                   'dr_flag'
                                   ]]
        analysis_3a_df = analysis_3a_df.loc[analysis_3a_df['subscription_type'] == "PROD"]
        analysis_3a_mots_df = analysis_3a_df.groupby(['mots_id', 'subscription_type']). \
            agg({'dr_flag': 'max'}).reset_index()
        analysis_3a_df = analysis_3a_df[['mots_id',
                                         'subscription_type']].drop_duplicates()
        analysis_3a_df = analysis_3a_df.merge(analysis_3a_mots_df,
                                              on=['mots_id', 'subscription_type'],
                                              how="inner")
        conditions = [(analysis_3a_df.dr_flag == 0) &
                      (analysis_3a_df['subscription_type'] == "PROD")]
        choices = ['3A']
        analysis_3a_df['derived_rti'] = np.select(conditions, choices, default=0)
        analysis_3a_df = analysis_3a_df.loc[analysis_3a_df.derived_rti.isin(['3A'])]
        analysis_3a_df.to_csv("analysis_3a_df.csv")
        analysis_df = pd.concat([analysis_0a_1a_1b_df,
                                 analysis_2a_2b_df,
                                 analysis_3a_df],
                                axis=0)
        derived_rti_df = analysis_df[['mots_id', 'derived_rti']].drop_duplicates()
        # Find MOTS IDs for which derived rti cannot be determined
        missing_derived_rti_df = final_df[['mots_id']].drop_duplicates() \
            .merge(derived_rti_df,
                   on=['mots_id'],
                   how='left',
                   indicator=True)
        missing_derived_rti_df = missing_derived_rti_df.rename(columns={'_merge': 'merge_indicator'})
        # Identify MOTS IDs which derived rti could not be determined
        missing_derived_rti_df = missing_derived_rti_df.loc[missing_derived_rti_df['merge_indicator'] ==
                                                            'left_only']
        missing_derived_rti_df = missing_derived_rti_df.drop(columns=['merge_indicator'])
        # Set derived rti to 0
        missing_derived_rti_df.loc[:, 'derived_rti'] = '0'
        # Concatenate missing rti with derived rt dataframe
        if not missing_derived_rti_df.empty:
            derived_rti_df = pd.concat([derived_rti_df, missing_derived_rti_df],
                                       axis=0)
        # Populate DR exercise time to calculate target rti
        derived_rti_df = derived_rti_df.merge(final_df[['mots_id',
                                                        'dr_exercise_time']].drop_duplicates(),
                                              on=["mots_id"],
                                              how="inner")
        # Converting derived rti dataframe to Json
        derived_rti_json = derived_rti_df.to_json(orient="records")
        compute_target_value = ComputeTargetRTValueUtility(self.sf_conn, self.project_name)
        derived_rti_df = compute_target_value.get_rt_parameters(derived_rti_json)
        return derived_rti_df

    @raise_exception_globally
    def fetch_data(self):
        df = dict()
        # Executing query to get app service configuration from snowflake
        self.logger.info("Fetch subscription configuration")
        df['subscription_details'] = self.subscription_df

        self.logger.info("Fetch Iaas DB config daily data")
        df['sf_iaas_db_config_daily'] = pd.DataFrame(self.fetch_sf_iaas_db_config_daily_data())
        self.logger.info(f"Columns in returned Config Daily Iaas DB DataFrame: {df['sf_iaas_db_config_daily'].columns.tolist()}")

        self.logger.info("Fetch Iaas Db metrics data")
        df['sf_iaas_db_metrics_daily'] = pd.DataFrame(self.fetch_sf_iaas_db_metrics_daily_data())
        self.logger.info(f"Columns in returned Metrics Iaas DB DataFrame: {df['sf_iaas_db_metrics_daily'].columns.tolist()}")

        self.resource_cost_config_df = self.sf_conn.query_to_df(
            consts_cost.iaasdb_cost_configuration_query.format(os.environ['SF_TABLE_SCHEMA']), params=())
        self.logger.info(f"Columns in returned Cost Config Iaas DB DataFrame: {self.resource_cost_config_df.columns.tolist()}")

        self.logger.info("Join dataframes")
        final_df = df['sf_iaas_db_config_daily']. \
            merge(df['sf_iaas_db_metrics_daily'],
                  on=["subscription_id", "resource_id"],
                  how="left",
            suffixes=("", "_m")). \
            merge(self.subscription_df,
                  on=["subscription_id"]). \
            merge(self.resource_cost_config_df,
                  on=["part_sku_size", "platform", "location"])

        final_df = final_df.rename(columns={'cost': 'vm_sku_cost'})
        if "mots_id" in final_df.columns:
            final_df['mots_id'] = final_df['mots_id'].astype(str).str.strip()
        else:
            self.logger.error("mots_id column missing in final_df after subscription merge.")
        # Invoke method that computes dr flag to mark regions as primary/dr
        self.logger.info(f"Columns in returned fetch data final df DataFrame: {final_df.columns.tolist()}")
        final_df = self.compute_dr_flag(final_df)
        self.logger.info(f"Columns in returned fetch data after compute_dr_flag final df DataFrame: {final_df.columns.tolist()}")
        # Compute measures based on dr flag
        final_df = self.generate_measures_based_on_dr_flag(final_df)
        if "mots_id" in final_df.columns:
            final_df['mots_id'] = final_df['mots_id'].astype(str).str.strip()
        else:
            self.logger.error("mots_id column missing in final_df after subscription merge.")
        self.logger.info(f"Columns in returned fetch data after generate_measures_based_on_dr_flag final df DataFrame: {final_df.columns.tolist()}")
        print("Final DF after DR measures (first 5 rows):")
        print(final_df.head())
        # input("Press Enter to continue with DR exercise time query...")

        final_df['mots_id'] = final_df['mots_id'].astype(str).str.strip()
        mots_id_list = final_df['mots_id'].drop_duplicates().to_list()
        mots_id_str = ",".join(mots_id_list)
        print("MOTS IDs:", mots_id_str)
        # Get DR exercise time
        if mots_id_str.strip() == "":
            self.logger.info("No MOTS IDs found. Creating empty DR exercise dataframe.")
            dr_exercise_df = pd.DataFrame(columns=["mots_id", "dr_exercise_time"])
        else:
            query = drdb_consts.dr_exercise_time_mins_calculate.format(mots_id_str)
            dr_exercise_df = self.sql_server_utility.query_to_df(query)
        # Cast dr_exercise_df 'mots_id' to string before merge.
        dr_exercise_df['mots_id'] = dr_exercise_df['mots_id'].astype(str).str.strip()
        # Populate DR exercise time
        final_df = final_df.merge(dr_exercise_df[['mots_id', 'dr_exercise_time']],
                                  on=["mots_id"],
                                  how="left")
        derived_rti_df = self.compute_derived_rt_value(final_df)
        final_df['mots_id'] = final_df['mots_id'].astype(str)
        final_df = final_df.merge(derived_rti_df[["mots_id",
                                                  "derived_rti",
                                                  "business_criticality",
                                                  "target_rt_code",
                                                  "rti_status"
                                                  ]],
                                  on=["mots_id"],
                                  how="left")
        # Setting incorrect provisioning flag for cases when incorrect provisioning will lead to cost savings
        conditions = [((final_df['target_rt_code'] == '3A') | (final_df['target_rt_code'] == '3B')) &
                      (final_df['rti_status'] == 'over-provisioned') &
                      (final_df['subscription_type'] == 'PROD') &
                      (final_df['dr_flag'] == 1)]
        choices = [1]
        final_df['invalid_provisioning_flag'] = np.select(conditions, choices, 0)
        # Identify expected number of workers
        # If the target rt code is 2A/2B and the region is a DR region,
        # then all App service plans within that region should have exactly 1 worker node
        conditions = [(final_df["target_rt_code"] == '2A/2B') &
                      (final_df["dr_flag"] == 1) &
                      (final_df['subscription_type'] == "PROD"),
                      (final_df["target_rt_code"] == '1B') &
                      (final_df["dr_flag"] == 1) &
                      (final_df['subscription_type'] == "PROD") &
                      (final_df['standby_mode'] == "warm")
                      ]
        choices = [1, 1]
        # final_df['expected_number_of_workers'] = np.select(conditions,
        #                                                    choices,
        #                                                    default=final_df['current_number_of_workers'])
        """
        conditions = [(final_df['cpu_utilization_dr'] >= 0.5*final_df['cpu_utilization_non_dr']) &
                      (final_df['subscription_name'].str.contains("-PROD-")),
                      (final_df['cpu_utilization_dr'] < 0.05 * final_df['cpu_utilization_non_dr']) &
                      (final_df['subscription_name'].str.contains("-PROD-")),
                      (final_df['dr_flag'] == 1) &
                      (final_df['state_flag'] == 0) &
                      (final_df['subscription_name'].str.contains("-PROD-")),
                      (final_df['dr_missing_flag'] == 1) &
                      (final_df['subscription_name'].str.contains("-PROD-")),
                      (final_df['subscription_name'].str.contains("-NPRD-"))
                      ]
        choices = ['0A', '1B', '2A/2B', '3A', 0]
        final_df['derived_rti'] = np.select(conditions, choices, default=0)
        """
        # Default null values
        final_df = final_df.fillna(0)
        return final_df


def main():
    from AzureUtils.azure_keyvault_utility import AzureKeyVaultUtility
    from AzureUtils.azure_data_utility import AzureDataUtility
    from DbUtils.snowflake_utility import SnowFlakeUtility
