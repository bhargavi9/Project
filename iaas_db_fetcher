    logger.info("Fetch Subscription id for a subscription name")
    subscription_df = azure_data_utility.fetch_subscription_df(subscription_name, reprocess_flag=False)
    process_date = datetime.now().strftime("%Y-%m-%d")
    with open("rt_values.json", "rb") as fl:
        data = json.load(fl)
    rt_values_df = pd.DataFrame(data)
    rt_values_df['mots_id'] = rt_values_df['mots_id'].astype(int)
    iaas_db_dr_configuration_fetcher = IaasDbDRConfigurationFetcher('Ask_DR_Project', sf_con,
                                                                           subscription_df, process_date, rt_values_df)
    final_df = iaas_db_dr_configuration_fetcher.fetch_data()
    final_df.to_csv("final.csv")
    logger.info("Close Snowflake database connection")
    sf_con.conn.close()


if __name__ == '__main__':
    logger = logging.getLogger('Ask_DR_Project')
    logger.setLevel(10)
    stream_handler = logging.StreamHandler()
    stream_handler.setLevel(10)
    formatter = logging.Formatter("%(asctime)s - %(name)s - %(levelname)s - %(message)s")
    stream_handler.setFormatter(formatter)
    logger.addHandler(stream_handler)
    main()
