                        'SF_PRIVATE_KEY_PASSPHRASE': 'sf-private-key-passphrase',
                        'SF_USER': 'sf-user',
                        'SF_ACCOUNT': 'sf-account',
                        'SF_WAREHOUSE': 'sf-warehouse',
                        'SF_DATABASE': 'sf-database',
                        'SF_SCHEMA': 'sf-schema',
                        'SF_TABLE_SCHEMA': 'sf-table-schema',
                        'SF_AO_TABLE_SCHEMA': 'sf-ao-table-schema',
                        'SQL_SERVER_HOST': 'sql-server-host',
                        'SQL_SERVER_USER': 'sql-server-user',
                        'SQL_SERVER_DATABASE': 'sql-server-database',
                        'SQL_SERVER_PASSWORD': 'sql-server-password'
                        }
    project_home = os.path.dirname(os.path.dirname(__file__))
    config_file_name = os.path.join(project_home, "askdrserver.conf")
    azure_key_vault_utility = AzureKeyVaultUtility("Ask_DR_Project", config_file_name, required_secrets)
    azure_key_vault_utility.load_keyvault_secrets_to_env()
    config = dict(os.environ)
    sf_con = SnowFlakeUtility(config)
    azure_data_utility = AzureDataUtility("Ask_DR_Project", sf_con)
    # derive subscription id from subscription name
