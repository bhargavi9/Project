# import sys
import os
import sys

sys.path.append(os.path.dirname(os.path.dirname(__file__)))
import Constants.constants_iaas_db_queries as consts
from DbUtils.snowflake_utility import SnowFlakeUtility
from ExceptionUtils.exception_handling import raise_exception_globally
from RecoveryTypeUtils.compute_target_rt_value_utility import ComputeTargetRTValueUtility
from DbUtils.sqlserver_utility import SqlServerUtility
import DbUtils.constants_drdb_queries as drdb_consts
import Constants.constants_resource_cost_configuration as consts_cost
import logging
import pandas as pd
import numpy as np


class IaasDbDRConfigurationFetcher:
    @raise_exception_globally
    def __init__(self, project_name: str, sf_conn: SnowFlakeUtility, subscription_df: pd.DataFrame,
                 process_date, rt_values_df):
        self.logger = logging.getLogger(project_name)
        self.project_name = project_name
        self.sf_conn = sf_conn
        self.subscription_df = subscription_df
        self.subscription_id = ",".join([f"'{element}'" for element in
                                         self.subscription_df['subscription_id'].tolist()])
        self.process_date = process_date
        self.rt_values_df = rt_values_df
        config = dict(os.environ)
        self.sql_server_utility = SqlServerUtility(conf=config, project_name="Ask DR server", sf_con=self.sf_conn)

    @raise_exception_globally
    def fetch_sf_iaas_db_config_daily_data(self) -> pd.DataFrame:
        return self.sf_conn.query_to_df(consts.iaas_db_config_daily_query.format(os.environ['SF_TABLE_SCHEMA'],
                                                                                     self.subscription_id,
                                                                                     self.process_date), params=())

    @raise_exception_globally
    def fetch_sf_iaas_db_metrics_daily_data(self) -> pd.DataFrame:
        """
        This method fetches the attributes of various storage accounts for a subscription from Snowflake

        :returns: Attributes of various storage accounts within a subscription
        :rtype: pd.DataFrame
        """
        return self.sf_conn.query_to_df(consts.iaas_db_metrics_daily_query.format(os.environ['SF_TABLE_SCHEMA'],
                                                                                      self.subscription_id,
                                                                                      self.process_date), params=())

    @raise_exception_globally
    def format_nondr_resource_name_column(self, data):
        if "-prod-" in data['subscription_name'].lower():
            return data['resource_name']. \
                replace("-prod-", ""). \
                replace("prod", ""). \
                replace(data['location'], "")
        elif "-nprd-" in data['subscription_name'].lower():
            return data['resource_name']. \
                replace("-nprd-", ""). \
                replace("-dev-", ""). \
                replace("-test-", ""). \
                replace(data['location'], "")

    @raise_exception_globally
    def format_dr_resource_name_column(self, data):
        return data['resource_name']. \
            replace("-dr-", ""). \
            replace("dr", ""). \
            replace("-prod-", ""). \
            replace("prod", ""). \
            replace(data['location'], "")

    @raise_exception_globally
    def reevaulate_mots_standby_df(self, non_determined_mots_standby_analysis_df):
        self.logger.info("Running the logic to re-evaluate standby df")
        self.logger.info("Running the logic to determine number of VMs per mots and region combination")
        count_vm_df = non_determined_mots_standby_analysis_df.groupby(['mots_id', 'location'])['resource_name']. \
            count().reset_index(name='count_vm')
        self.logger.info("Running the logic to determine number of regions per mots")
        count_regions_df = non_determined_mots_standby_analysis_df[['mots_id', 'location']]. \
            drop_duplicates().groupby(['mots_id'])['location']. \
            count().reset_index(name='count_regions')
        # Combining the count measures with the original dataframe
        non_determined_mots_standby_analysis_df = non_determined_mots_standby_analysis_df.merge(count_vm_df,
                                                                                                on=["mots_id",
                                                                                                    "location"],
                                                                                                how="left"
                                                                                                ). \
            merge(count_regions_df,
                  on=["mots_id"],
                  how="left")
        # Analyze those MOTS for which standby information can be determined
        # SKUs can be compared between primary and DR regions for cases when there are 2 regions for a MOTS and each region has 1 vm
        non_determined_mots_standby_analysis_df = non_determined_mots_standby_analysis_df.loc[
            (non_determined_mots_standby_analysis_df['count_vm'] == 1) &
            (non_determined_mots_standby_analysis_df['count_regions'] == 2)]
        dr_df = non_determined_mots_standby_analysis_df.loc[non_determined_mots_standby_analysis_df['dr_flag'] == 1]
        non_dr_df = non_determined_mots_standby_analysis_df.loc[non_determined_mots_standby_analysis_df['dr_flag'] == 0]
        join_df = non_dr_df.merge(dr_df,
                                  on=["mots_id", "subscription_id"],
                                  how="outer",
                                  suffixes=['_non_dr', '_dr'])
        reevaulated_mots_standby_df = self.compute_standby_info(join_df)
        reevaluated_invalid_standby_df = self.compute_invalid_standby_info(join_df)

        return reevaulated_mots_standby_df, reevaluated_invalid_standby_df

    @raise_exception_globally
    def compute_invalid_standby_info(self, analysis_df):
        analysis_df = analysis_df.copy()
        conditions = [(~ analysis_df['resource_name_non_dr'].isna())
                      &
                      (~ analysis_df['resource_name_dr'].isna())
                      &
                      (analysis_df['vm_sku_cost_dr'] > analysis_df['vm_sku_cost_non_dr'])]
        choices = ["invalid"]
        analysis_df['standby_mode'] = np.select(conditions, choices, default=0)
        invalid_standby_df = analysis_df.loc[analysis_df['standby_mode'] == 'invalid']
        invalid_standby_df = invalid_standby_df[['mots_id',
                                                 'subscription_id',
                                                 'resource_name_dr',
                                                 'sku_name_non_dr']].drop_duplicates()
        invalid_standby_df = invalid_standby_df.rename(columns={'resource_name_dr': 'resource_name',
                                                                'sku_name_non_dr': 'expected_sku_name'})
        invalid_standby_df['invalid_standby_flag'] = 1
        return invalid_standby_df

    # @raise_exception_globally
    # def compute_invalid_number_of_workers_info(self, analysis_df):
    #     # Mark app service plans with dr workers count greater than prod workers count
